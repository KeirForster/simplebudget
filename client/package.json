{
  "name": "client",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "build": "ng build default",
    "build:default:production": "ng build --prod",
    "e2e": "run-s e2e:default",
    "e2e:default": "cypress open --project ./projects/default/e2e",
    "format": "prettier --config ./.prettierrc --write \"projects/*/src/{app,environments,assets}/**/*{.ts,.js,.json,.css,.scss}\"",
    "format:check": "prettier --config ./.prettierrc --list-different \"projects/*/src/{app,environments,assets}/**/*{.ts,.js,.json,.css,.scss}\"",
    "format:fix": "pretty-quick --staged",
    "lint": "run-s lint:default",
    "lint:default": "ng lint default",
    "precommit": "run-s format:fix lint",
    "start:default": "ng serve default",
    "start:prod": "live-server dist/default",
    "test": "run-s test:default",
    "test:default": "jest --config ./projects/default/jest.config.js",
    "test:coverage": "run-s test:coverage:default",
    "test:coverage:default": "npm run test:default -- --coverage"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^11.0.1",
    "@angular/common": "^11.0.1",
    "@angular/compiler": "^11.0.1",
    "@angular/core": "^11.0.1",
    "@angular/forms": "^11.0.1",
    "@angular/platform-browser": "^11.0.1",
    "@angular/platform-browser-dynamic": "^11.0.1",
    "@angular/router": "^11.0.1",
    "rxjs": "~6.6.0",
    "tslib": "^2.0.0",
    "zone.js": "^0.11.3"
  },
  "devDependencies": {
    "@angular-builders/custom-webpack": "^10.0.1",
    "@angular-devkit/build-angular": "^0.1100.2",
    "@angular/cli": "^11.0.2",
    "@angular/compiler-cli": "^11.0.1",
    "@tailwindcss/ui": "^0.7.2",
    "@types/jest": "^26.0.15",
    "@types/node": "^12.19.5",
    "autoprefixer": "^10.0.2",
    "codelyzer": "^6.0.0",
    "cypress": "^5.6.0",
    "dotenv": "^8.2.0",
    "husky": "^4.3.0",
    "jest": "^26.6.3",
    "jest-preset-angular": "^8.3.2",
    "live-server": "^1.2.1",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.1.7",
    "postcss-import": "^13.0.0",
    "postcss-loader": "^4.0.4",
    "postcss-scss": "^3.0.4",
    "prettier": "^2.1.2",
    "pretty-quick": "^3.1.0",
    "stylelint-config-recommended": "^3.0.0",
    "tailwindcss": "^2.0.1",
    "ts-node": "^9.0.0",
    "tslint": "~6.1.0",
    "typescript": "^4.0.5"
  }
}
