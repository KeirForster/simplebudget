{
  "name": "client",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "build": "ng build default",
    "build:default:production": "ng build --prod",
    "e2e": "run-s e2e:default",
    "e2e:default": "cypress open --project ./projects/default/e2e",
    "format": "prettier --config ./.prettierrc --write \"projects/*/src/{app,environments,assets}/**/*{.ts,.js,.json,.scss}\"",
    "format:check": "prettier --config ./.prettierrc --list-different \"projects/*/src/{app,environments,assets}/**/*{.ts,.js,.json,.css,.scss}\"",
    "format:fix": "pretty-quick --staged",
    "lint": "run-s lint:default",
    "lint:default": "ng lint default",
    "precommit": "run-s format:fix lint",
    "start:default": "ng serve default",
    "start:prod": "live-server dist/default",
    "test": "run-s test:default",
    "test:default": "jest --config ./projects/default/jest.config.js",
    "test:coverage": "run-s test:coverage:default",
    "test:coverage:default": "npm run test:default -- --coverage"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^11.2.3",
    "@angular/common": "^11.2.3",
    "@angular/compiler": "^11.2.3",
    "@angular/core": "^11.2.3",
    "@angular/forms": "^11.2.3",
    "@angular/platform-browser": "^11.2.3",
    "@angular/platform-browser-dynamic": "^11.2.3",
    "@angular/router": "^11.2.3",
    "rxjs": "^6.6.6",
    "tslib": "^2.1.0",
    "zone.js": "^0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.1102.2",
    "@angular-eslint/builder": "^1.2.0",
    "@angular-eslint/eslint-plugin": "^1.2.0",
    "@angular-eslint/eslint-plugin-template": "^1.2.0",
    "@angular-eslint/schematics": "^1.2.0",
    "@angular-eslint/template-parser": "^1.2.0",
    "@angular/cli": "^11.2.2",
    "@angular/compiler-cli": "^11.2.3",
    "@tailwindcss/ui": "^0.7.2",
    "@types/jest": "^26.0.20",
    "@types/node": "^14.14.31",
    "@typescript-eslint/eslint-plugin": "^4.15.2",
    "@typescript-eslint/parser": "^4.15.2",
    "autoprefixer": "^10.2.4",
    "codelyzer": "^6.0.0",
    "cypress": "^6.5.0",
    "dotenv": "^8.2.0",
    "eslint": "^7.20.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsdoc": "^32.2.0",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "husky": "^5.1.1",
    "jest": "^26.6.3",
    "jest-preset-angular": "^8.3.2",
    "live-server": "^1.2.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.2.1",
    "pretty-quick": "^3.1.0",
    "stylelint-config-recommended": "^3.0.0",
    "tailwindcss": "^2.0.3",
    "ts-node": "^9.1.1",
    "tslint": "~6.1.0",
    "typescript": "^4.1.5"
  }
}
